{
    "name": "mortenscheel/php-inquirer",
    "description": "Beautify command-line prompts",
    "autoload": {
        "psr-4": {
            "Scheel\\Inquirer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.1",
        "laravel/pint": "^1.16",
        "phpstan/phpstan": "^1.11",
        "ergebnis/composer-normalize": "^2.43"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "scripts": {
        "check": "phpstan analyse src --level 9"
    }
}
